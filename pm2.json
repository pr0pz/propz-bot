{
	"apps": [{
		"name": "propzbot",
		"script": "./twitch-bot/index.ts",
		"interpreter": "deno",
		"interpreter_args": "run --allow-env --allow-read --allow-write --allow-net --allow-sys --allow-run --unstable-cron --env-file",
		"max_memory_restart": "350M",
		"instances": 1,
		"autorestart": true,
		"watch": false,
		"env": {
			"NODE_ENV": "production"
		},
		"log_date_format": "YYYY-MM-DD HH:mm:ss Z",
		"merge_logs": true,
		"out_file": "./logs/propzbot-out.log",
		"error_file": "./logs/propzbot-error.log"
	}]
}
